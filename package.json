      "apisauce": "1.1.1",
      "autoprefixer": "^9.7.5",
      "babel-plugin-macros": "^2.8.0",
      "js-cookie": "2.2.1",
      "material-ui": "0.20.2",
      "mobx": "5.15.0",
      "mobx-react": "6.1.4",
      "model": "6.0.1",
      "postcss-cli": "^7.1.0",
      "rc-menu": "8.3.1",
      "rc-progress": "3.0.0",
      "react": "16.13.1",
      "react-dom": "^16.13.0",
      "react-icons": "3.10.0",
      "react-loading": "2.0.3",
      "react-modal": "3.11.2",
      "react-model": "3.1.2",
      "react-native-options-menu": "2.0.2",
      "react-progressbar": "15.4.1",
      "react-router-dom": "^5.1.2",
      "react-scripts": "3.4.0",
      "react-toast-notifications": "2.4.0",
      "react-toastify": "6.0.5",
      "semantic-ui-react": "0.88.2",
      "tailwind.macro": "^1.0.0-alpha.10",
      "typescript": "3.8.3"
   },
   "scripts": {
      "build:css": "postcss src/index.css -o src/styles/tailwind.css",
      "watch:css": "postcss src/index.css -o src/styles/tailwind.css",
      "start": "npm run watch:css & react-app-rewired start",
      "build": "npm run build:css & react-app-rewired build",
      "test": "react-app-rewired test",
      "eject": "react-app-rewired eject",
      "ts:start": "npm run watch:css & react-scripts start",
      "ts:build": "npm run build:css & react-scripts build",
      "ts:test": "react-scripts test --env=jest-environment-jsdom-sixteen",
      "ts:eject": "react-scripts eject",
      "deploy": "export PUBLIC_URL=/2019/$C9_USER/react && npm run build && ./deploy.sh",
      "storybook": "start-storybook -p 8080 -s public",
      "build-storybook": "build-storybook -s public"
   },
   "eslintConfig": {
      "extends": "react-app"
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "@babel/core": "7.7.2",
      "@babel/plugin-proposal-decorators": "7.7.0",
      "@babel/plugin-syntax-jsx": "7.2.0",
      "@babel/preset-env": "7.7.1",
      "@storybook/addon-actions": "5.3.18",
      "@storybook/addon-docs": "5.3.18",
      "@storybook/addon-knobs": "5.3.18",
      "@storybook/addon-links": "5.3.18",
      "@storybook/addon-storysource": "5.3.18",
      "@storybook/addons": "5.3.18",
      "@storybook/preset-create-react-app": "2.1.2",
      "@storybook/react": "5.3.18",
      "@testing-library/jest-dom": "5.5.0",
      "@testing-library/react": "10.0.3",
      "babel-eslint": "10.0.3",
      "customize-cra": "0.9.1",
      "jest-environment-jsdom-sixteen": "1.0.3",
      "react-app-rewired": "2.1.5",
      "tailwindcss": "^1.2.0"
   },
   "jest": {
      "testEnvironment": "node",
      "collectCoverageFrom": [
         "!src/App.{ts,tsx,js,jsx}",
         "!src/index.{ts,tsx,js,jsx}",
         "!src/serviceWorker.{ts,tsx,js,jsx}"
      ]
   }
}
